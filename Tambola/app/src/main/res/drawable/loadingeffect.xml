<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <stroke
                android:width="2dp"
                android:height="2dp"
                android:color="@color/colorPrimaryDark" />
            <solid
                android:color="#AED6F1" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
            <size
                android:height="120dp"
                android:width="120dp"
                />
        </shape>
    </item>

</layer-list>